{
  "name": "araks",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@tanstack/react-query": "^4.27.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "antd": "^5.3.1",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icomoon": "^2.5.7",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.9",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "prepare": "husky install",
    "pre-commit": "npm-run-all --parallel type-check lint-staged",
    "type-check": "tsc --noEmit",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./",
    "lint-fix": "eslint ./ --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\""
  },
  "lint-staged": {
    "src/**/*.{js, jsx, json, ts, tsx}": [
      "eslint --cache"
    ],
    "src/**/*.{js, jsx, json, css, md, ts, tsx}": [
      "prettier --write --cache"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && format"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.6"
  }
}
